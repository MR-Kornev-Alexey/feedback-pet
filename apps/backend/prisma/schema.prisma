generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id        String   @id @default(uuid())
  stars     Int
  text      String
  customer  String
  orderLine Int
  urlImage  String
  show      Boolean  @default(true)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
